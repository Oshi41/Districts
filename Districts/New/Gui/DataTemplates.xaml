<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Districts.New.Gui"
                    xmlns:viewModel="clr-namespace:Districts.New.ViewModel"
                    xmlns:controls="clr-namespace:Districts.New.Gui.Controls">

    <DataTemplate DataType="{x:Type viewModel:EditStreetViewModel}">
        <controls:EditStreets/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModel:MainViewModel}">
        <controls:MainView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModel:EditHomesViewModel}">
        <controls:EditHomes/>
    </DataTemplate>

</ResourceDictionary>